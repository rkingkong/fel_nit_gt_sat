            <field name="is_debit_note">False</field>
            <field name="requires_reference">False</field>
            <field name="allowed_regimes">general,especial</field>
            <field name="xml_ns">http://www.sat.gob.gt/dte/fel/0.1.0</field>
            <field name="sequence">70</field>
            <field name="is_active">True</field>
        </record>
        
        <!-- RECI - Recibo -->
        <record id="fel_document_type_reci" model="fel.document.type">
            <field name="name">Recibo</field>
            <field name="code">RECI</field>
            <field name="description">Recibo por Pago</field>
            <field name="is_invoice">False</field>
            <field name="is_credit_note">False</field>
            <field name="is_debit_note">False</field>
            <field name="requires_reference">False</field>
            <field name="allowed_regimes">general,especial</field>
            <field name="xml_ns">http://www.sat.gob.gt/dte/fel/0.1.0</field>
            <field name="sequence">80</field>
            <field name="is_active">True</field>
        </record>
        
        <!-- RDON - Recibo por Donación -->
        <record id="fel_document_type_rdon" model="fel.document.type">
            <field name="name">Recibo por Donación</field>
            <field name="code">RDON</field>
            <field name="description">Recibo por Donación Deducible</field>
            <field name="is_invoice">False</field>
            <field name="is_credit_note">False</field>
            <field name="is_debit_note">False</field>
            <field name="requires_reference">False</field>
            <field name="allowed_regimes">general</field>
            <field name="xml_ns">http://www.sat.gob.gt/dte/fel/0.1.0</field>
            <field name="sequence">90</field>
            <field name="is_active">True</field>
        </record>
        
        <!-- Standard Tax Phrases -->
        
        <!-- ISR Phrase -->
        <record id="fel_tax_phrase_isr" model="fel.tax.phrase">
            <field name="name">Sujeto a retención definitiva ISR</field>
            <field name="phrase_type">1</field>
            <field name="scenario_code">1</field>
            <field name="description">Para facturas con retención de ISR</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="is_active">True</field>
        </record>
        
        <!-- IVA Phrase -->
        <record id="fel_tax_phrase_iva" model="fel.tax.phrase">
            <field name="name">Sujeto a pagos trimestrales ISR</field>
            <field name="phrase_type">1</field>
            <field name="scenario_code">2</field>
            <field name="description">Para régimen de pequeño contribuyente</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="is_active">True</field>
        </record>
        
    </data>
</odoo>